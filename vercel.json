{
  "builds": [
    {
      "src": "BlazorHybridSPA/BlazorHybridSPA.csproj",
      "use": "@vercel/dotnet",
      "config": {
        "commands": {
          "build": "dotnet publish -c Release -o ./out"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/out/$1"
    }
  ]
}
